{
  "name": "rss-reader-infrastructure",
  "version": "1.0.0",
  "description": "RSS Reader AWS CDK Infrastructure",
  "bin": {
    "infrastructure": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext ts --max-warnings 0",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=development",
    "deploy:prod": "cdk deploy --context environment=production",
    "destroy": "cdk destroy",
    "destroy:dev": "cdk destroy --context environment=development",
    "destroy:prod": "cdk destroy --context environment=production",
    "diff": "cdk diff",
    "diff:dev": "cdk diff --context environment=development",
    "diff:prod": "cdk diff --context environment=production",
    "synth": "cdk synth",
    "synth:dev": "cdk synth --context environment=development",
    "synth:prod": "cdk synth --context environment=production",
    "bootstrap": "cdk bootstrap",
    "bootstrap:dev": "cdk bootstrap --context environment=development",
    "bootstrap:prod": "cdk bootstrap --context environment=production"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@types/jest": "^29.5.8",
    "@types/node": "20.8.10",
    "eslint": "^8.53.0",
    "aws-cdk": "2.1100.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.233.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
